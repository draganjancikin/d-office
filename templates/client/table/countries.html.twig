<table class="table table-bordered table-hover dataTable" id="" width="100%" cellspacing="0">
    <thead class="thead-light"><tr><th>Država</th><th>Skraćenica</th></tr></thead>
    <tbody>
    {% for country in all_countries %}
        <tr>
            <td><a href="/countries/{{ country.id }}">{{ country.name }}</a></td>
            <td>{{ country.abbr }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
